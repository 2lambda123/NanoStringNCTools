### - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
### Export S4 generics

export("weights<-",
       NanoStringRccSet,
       sData, svarLabels,
       signatures, "signatures<-", signatureScores,
       endogenousSubset, housekeepingSubset,
       negativeControlSubset, positiveControlSubset,
